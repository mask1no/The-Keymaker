# ==============================================================================
# THE KEYMAKER - PRODUCTION CONFIGURATION TEMPLATE
# ==============================================================================
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control!

# ==============================================================================
# CORE SECURITY (REQUIRED)
# ==============================================================================
ENGINE_API_TOKEN=dev_token_min_32_chars_change_in_production_use_strong_random
KEYMAKER_SESSION_SECRET=dev_session_secret_min_32_chars_change_prod_use_strong_random
KEYPAIR_JSON=./keypairs/dev-payer.json

# ==============================================================================
# RPC ENDPOINTS (REQUIRED)
# ==============================================================================
HELIUS_RPC_URL=https://api.mainnet-beta.solana.com
NEXT_PUBLIC_HELIUS_RPC=https://api.mainnet-beta.solana.com
JITO_RPC_URL=https://mainnet.block-engine.jito.wtf/api/v1
PUBLIC_RPC_URL=https://api.mainnet-beta.solana.com
HELIUS_RPC_DEVNET_URL=https://api.devnet.solana.com

# ==============================================================================
# REDIS (REQUIRED FOR PRODUCTION)
# ==============================================================================
UPSTASH_REDIS_REST_URL=https://example.upstash.io
UPSTASH_REDIS_REST_TOKEN=example_token_replace_with_your_upstash_token

# ==============================================================================
# API KEYS (OPTIONAL BUT RECOMMENDED)
# ==============================================================================
JUPITER_API_KEY=optional_but_recommended_for_production
BIRDEYE_API_KEY=optional_for_token_data_and_analytics
JITO_AUTH_TOKEN=optional_for_jito_priority_access

# ==============================================================================
# SAFETY CONTROLS (CRITICAL)
# ==============================================================================
KEYMAKER_DISABLE_LIVE=YES
KEYMAKER_REQUIRE_ARMING=YES
DRY_RUN=true

# ==============================================================================
# NETWORK CONFIGURATION
# ==============================================================================
NETWORK=mainnet-beta
NEXT_PUBLIC_NETWORK=mainnet-beta

# ==============================================================================
# EXECUTION SETTINGS
# ==============================================================================
KEYMAKER_GROUP=default
KEYMAKER_AMOUNT_LAMPORTS=1000000
NEXT_PUBLIC_BUNDLE_TX_LIMIT=5

# ==============================================================================
# RATE LIMITING
# ==============================================================================
SUBMIT_MAX_RPM=10
PAYLOAD_LIMIT_SUBMIT_BYTES=32768
SUBMIT_MAX_DELAY_SECONDS=120
SUBMIT_MAX_TXS=5

# ==============================================================================
# SERVER CONFIGURATION
# ==============================================================================
PORT=3000
NODE_ENV=development

# ==============================================================================
# FEATURE FLAGS
# ==============================================================================
LEGACY_ENABLED=false
SIWS_AUTH_ENABLED=true
PHANTOM_AUTO_CONNECT=true
ENABLE_SLOT_TARGETING=false

# ==============================================================================
# MONITORING & LOGGING (OPTIONAL)
# ==============================================================================
LOG_LEVEL=info

# ==============================================================================
# DATABASE (OPTIONAL - uses SQLite by default)
# ==============================================================================
ANALYTICS_DB_PATH=./data/analytics.db
KEYMAKER_DB_PATH=./data/keymaker.db
