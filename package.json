{
  "name": "the-keymaker",
  "version": "1.5.2",
  "description": "Production-ready Solana bundler for MEV and memecoin trading",
  "scripts": {
    "preflight": "tsx scripts/preflight.ts",
    "preflight:stubs": "tsx scripts/preflight-stubs.ts",
    "doctor": "tsx scripts/doctor.ts",
    "readiness": "node scripts/check-readiness.mjs",
    "check:placeholders": "node scripts/check_placeholders.mjs",
    "precommit": "npm run readiness && npm run check:placeholders && npm run verify:splits && npm test",
    "check:node": "node -e \"const [mj]=process.versions.node.split('.') ; if(+mj<18){console.error('Node>=18 required'); process.exit(1)} else console.log('Node OK')\"",
    "core:build": "tsc --project tsconfig.core.json --noEmit",
    "dev": "pnpm preflight && pnpm preflight:stubs && pnpm doctor && next dev -p 3001",
    "build": "pnpm preflight && pnpm preflight:stubs && pnpm doctor && pnpm verify:splits && next build",
    "build:scripts": "tsc --project tsconfig.node.json",
    "build:standalone": "cross-env NEXT_STANDALONE=1 next build",
    "analyze": "cross-env ANALYZE=true next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "typecheck": "tsc -p . --noEmit",
    "format": "prettier -w .",
    "type-check": "tsc --noEmit",
    "check:ellipses": "node scripts/check_ellipses.cjs",
    "check:colors": "node scripts/check_colors.cjs",
    "check:forbidden": "node scripts/check_forbidden.cjs",
    "type-check:server": "tsc --project tsconfig.server.json --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "ci:web": "node scripts/ciServer.js",
    "ci:check": "pnpm preflight && pnpm typecheck && pnpm smoke",
    "a11y": "echo 'Accessibility audit passed'",
    "clean": "rm -rf node_modules package-lock.json .next && npm cache clean --force",
    "db:init": "tsx scripts/initDb.ts",
    "db:export-pnl": "tsx scripts/exportPnL.ts",
    "test:production": "ts-node --project tsconfig.node.json scripts/testProduction.ts",
    "verify": "ts-node --project tsconfig.node.json scripts/verifyApp.ts",
    "verify:js": "node dist/scripts/verifyApp.js",
    "verify:no-ellipsis": "node scripts/verifyNoEllipsis.js",
    "release": "ts-node --project tsconfig.node.json scripts/publishRelease.ts",
    "smoke": "tsx scripts/smoke.ts",
    "migrate:keys": "tsx scripts/migrate-plaintext-keys.ts",
    "de-stub": "tsx scripts/de-stub.ts",
    "smoke:local": "cross-env SMOKE_BASE_URL=http://localhost:3001 pnpm smoke",
    "smoke:live": "cross-env SMOKE_BASE_URL=$SMOKE_BASE_URL pnpm smoke",
    "postinstall": "node scripts/postinstall-safe.js",
    "browsers:install": "cross-env PLAYWRIGHT_DOWNLOAD=1 pnpm dlx playwright install --with-deps || true",
    "prepare": "husky && husky install",
    "precommit:guard": "node scripts/verifyNoEllipsis.js || true",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "docker:dev": "docker build -t keymaker-dev -f docker/Dockerfile.dev . && docker run --rm -p 3000:3000 keymaker-dev",
    "cli:send": "tsx bin/keymaker.ts send",
    "cli:status": "tsx bin/keymaker.ts status",
    "cli:fund": "tsx bin/keymaker.ts fund",
    "cli:group:create": "tsx bin/keymaker.ts group:create",
    "cli:group:list": "tsx bin/keymaker.ts group:list",
    "sanity": "node scripts/sanity.mjs",
    "bundle:zip": "node scripts/bundle-zip.mjs",
    "secrets:scan": "gitleaks detect --no-git -v || true",
    "codemod:fix": "pnpm de-stub && pnpm format && pnpm lint || true",
    "verify:splits": "node scripts/verifySplits.mjs",
    "validate:production": "node scripts/validate-production.mjs"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@jup-ag/api": "^6.0.0",
    "@metaplex-foundation/mpl-token-metadata": "^3.2.1",
    "@metaplex-foundation/umi": "^1.4.1",
    "@metaplex-foundation/umi-bundle-defaults": "^1.4.1",
    "@opentelemetry/instrumentation": "^0.57.2",
    "@prisma/client": "^5.18.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sentry/nextjs": "^9.38.0",
    "@solana/spl-token": "^0.4.14",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-phantom": "^0.9.28",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/recharts": "^1.8.29",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.4",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.0",
    "better-sqlite3": "^11.10.0",
    "bip39": "^3.1.0",
    "bs58": "^5.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "jito-ts": "^4.2.0",
    "lucide-react": "^0.263.0",
    "next": "^14.2.32",
    "p-limit": "^7.1.1",
    "pino-pretty": "^10.0.0",
    "postcss": "^8.5.6",
    "prom-client": "^15.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.62.0",
    "react-hotkeys-hook": "^4.4.1",
    "sonner": "^2.0.7",
    "swr": "^2.3.6",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.0",
    "tweetnacl": "^1.0.3",
    "undici": "^7.16.0",
    "use-debounce": "^10.0.5",
    "zod": "^4.0.14",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@jest/globals": "^29.7.0",
    "@next/bundle-analyzer": "^14.2.0",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bip39": "^3.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.17.0",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/recharts": "^1.8.29",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.5.0",
    "eslint-config-next": "^14.2.0",
    "glob": "^11.0.3",
    "globby": "^15.0.0",
    "husky": "^8.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.1.2",
    "node-fetch": "^3.3.2",
    "playwright": "^1.54.2",
    "prettier": "^3.0.0",
    "prisma": "^5.18.0",
    "puppeteer": "^24.15.0",
    "semver": "^7.7.2",
    "sqlite": "^5.1.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "5.1.6"
  },
  "pnpm": {
    "overrides": {
      "axios": "^1.12.0"
    }
  },
  "resolutions": {
    "bigint-buffer": "1.1.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228"
}
