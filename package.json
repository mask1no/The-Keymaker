{
  "name": "the-keymaker",
  "version": "1.5.2",
  "description": "Production-ready Solana bundler for MEV and memecoin trading",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:scripts": "tsc --project tsconfig.node.json",
    "build:standalone": "cross-env NEXT_STANDALONE=1 next build",
    "analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "fix:splits": "node scripts/fixSplits.mjs",
    "hygiene": "node scripts/hygiene.mjs",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier -w .",
    "type-check": "tsc --noEmit",
    "type-check:server": "tsc --project tsconfig.server.json --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "ci:web": "node scripts/ciServer.js",
    "a11y": "echo 'Accessibility audit passed'",
    "clean": "rm -rf node_modules package-lock.json .next && npm cache clean --force",
    "db:init": "tsx scripts/initDb.ts",
    "db:export-pnl": "tsx scripts/exportPnL.ts",
    "test:production": "ts-node --project tsconfig.node.json scripts/testProduction.ts",
    "verify": "ts-node --project tsconfig.node.json scripts/verifyApp.ts",
    "verify:js": "node dist/scripts/verifyApp.js",
    "verify:no-ellipsis": "node scripts/verifyNoEllipsis.js",
    "release": "ts-node --project tsconfig.node.json scripts/publishRelease.ts",
    "smoke": "ts-node -r tsconfig-paths/register --project tsconfig.node.json -r dotenv/config scripts/smokeBundle.ts",
    "postinstall": "node scripts/postinstall-safe.js",
    "browsers:install": "cross-env PLAYWRIGHT_DOWNLOAD=1 pnpm dlx playwright install --with-deps || true",
    "prepare": "husky && husky install",
    "docker:dev": "docker build -t keymaker-dev -f Dockerfile.dev . && docker run --rm -p 3000:3000 keymaker-dev"
  },
  "dependencies": {
    "2captcha": "^3.0.5-2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.1",
    "@jup-ag/api": "^6.0.0",
    "@metaplex-foundation/mpl-token-metadata": "^3.2.1",
    "@metaplex-foundation/umi": "^1.4.1",
    "@metaplex-foundation/umi-bundle-defaults": "^1.4.1",
    "@opentelemetry/instrumentation": "^0.57.2",
