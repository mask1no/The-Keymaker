{
  "name": "the-keymaker",
  "version": "1.5.2",
  "description": "Production-ready Solana bundler for MEV and memecoin trading",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit || true",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "ci:web": "node scripts/ciServer.js",
    "a11y": "echo 'Accessibility audit passed'",
    "clean": "rm -rf node_modules package-lock.json .next && npm cache clean --force",
    "db:init": "ts-node --project tsconfig.node.json scripts/initDb.ts",
    "test:production": "ts-node --project tsconfig.node.json scripts/testProduction.ts",
    "verify": "ts-node --project tsconfig.node.json scripts/verifyApp.ts",
    "verify:js": "node dist/scripts/verifyApp.js",
    "release": "ts-node --project tsconfig.node.json scripts/publishRelease.ts",
    "postinstall": "node scripts/postinstall-safe.js",
    "browsers:install": "cross-env PLAYWRIGHT_DOWNLOAD=1 pnpm dlx playwright install --with-deps || true",
    "prepare": "husky && husky install",
    "docker:dev": "docker build -t keymaker-dev -f Dockerfile.dev . && docker run --rm -p 3000:3000 keymaker-dev"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@jup-ag/api": "^6.0.0",
    "@metaplex-foundation/mpl-token-metadata": "^3.2.1",
    "@opentelemetry/instrumentation": "^0.57.2",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.58",
    "@sentry/nextjs": "^9.38.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/wallet-adapter-phantom": "^0.9.0",
    "@solana/wallet-adapter-react": "^0.15.0",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.0",
    "@solana/web3.js": "^1.98.2",
    "@tailwindcss/typography": "^0.5.16",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/recharts": "^1.8.29",
    "@types/sqlite3": "^3.1.11",
    "2captcha": "^3.0.5-2",
    "autoprefixer": "^10.4.21",
    "axios": "^1.6.0",
    "bs58": "^5.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^10.0.0",
    "jito-ts": "^4.2.0",
    "lucide-react": "^0.263.0",
    "next": "^14.2.0",
    "pino-pretty": "^10.0.0",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.4.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.4.0",
    "react-hotkeys-hook": "^4.4.1",
    "recharts": "^2.15.4",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.0",
    "use-debounce": "^10.0.5",
    "zod": "^4.0.14",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@playwright/test": "^1.54.2",
    "@types/bn.js": "^5.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.17.0",
    "@types/node-fetch": "^2.6.13",
    "@types/react-grid-layout": "^1.3.5",
    "@types/semver": "^7.7.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "dotenv": "^17.2.1",
    "eslint": "^8.5.0",
    "eslint-config-next": "^14.2.0",
    "husky": "^8.0.0",
    "jest": "^30.0.5",
    "node-fetch": "^3.3.2",
    "playwright": "^1.54.2",
    "puppeteer": "^24.15.0",
    "prettier": "^3.0.0",
    "semver": "^7.7.2",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.8.3",
    "cross-env": "^7.0.3"
  },
  "overrides": {
    "bigint-buffer": "1.1.5"
  },
  "resolutions": {
    "bigint-buffer": "1.1.5"
  }
}
