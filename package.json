    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/recharts": "^1.8.29",
    "@types/sqlite3": "^3.1.11",
    "autoprefixer": "^10.4.21",
    "axios": "^1.6.0",
    "bip39": "^3.1.0",
    "bs58": "^5.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
