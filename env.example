# ============================================
# The Keymaker Environment Configuration
# ============================================
# Copy this file to .env.local and fill in your values

# ============================================
# RPC ENDPOINTS
# ============================================
# Get a free API key at https://helius.dev
NEXT_PUBLIC_HELIUS_RPC=https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_KEY
HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_KEY
HELIUS_API_KEY=your_helius_api_key_here

# ============================================
# API KEYS (Backend only - never expose these)
# ============================================
# Birdeye - Get at https://birdeye.so
BIRDEYE_API_KEY=your_birdeye_api_key_here

# Jupiter - Get at https://station.jup.ag/docs
JUPITER_API_KEY=your_jupiter_api_key_here

# Jito - Get at https://jito.wtf
JITO_AUTH_TOKEN=your_jito_auth_token_here

# ============================================
# PLATFORM APIS (Optional)
# ============================================
# Pump.fun - Contact their team
PUMPFUN_API_KEY=your_pumpfun_api_key_here

# Moonshot - Get at https://moonshot.cc
MOONSHOT_API_KEY=your_moonshot_api_key_here
MOONSHOT_API_URL=https://api.moonshot.gg

# ============================================
# SOLANA CONFIGURATION
# ============================================
# Your Solana keypair in base58 format (for Python backend)
# Generate with: solana-keygen new --no-bip39-passphrase
SOLANA_KEYPAIR=your_base58_encoded_keypair_here

# Network (mainnet-beta or devnet)
NEXT_PUBLIC_NETWORK=mainnet-beta

# ============================================
# JITO CONFIGURATION
# ============================================
# Jito tip accounts (mainnet)
JITO_TIP_ACCOUNTS=96gYZGLnJYVFmbjzopPSU6QiEV5fGqZNyN9nmNhvrZU5,HFqU5x63VTqvQss8hp11i4wVV8bD44PvwucfZ2bU7gRe

# Default tip amount in lamports (0.01 SOL = 10000000)
NEXT_PUBLIC_JITO_TIP_AMOUNT=10000000

# ============================================
# OPTIONAL: ERROR TRACKING
# ============================================
# Sentry DSN for error tracking
NEXT_PUBLIC_SENTRY_DSN=
SENTRY_ORG=
SENTRY_PROJECT=
SENTRY_AUTH_TOKEN=

# ============================================
# DEVELOPMENT SETTINGS
# ============================================
# Set to true to enable debug logging
DEBUG=false

# Port for development server
PORT=3000 